{
    "id": "/simple-webserver",
    "cmd": "cd simple-webserver-v0.0.1-linux-amd64 && chmod +x simple-webserver && ./simple-webserver --listen \":$PORT0\"",
    "args": null,
    "cpus": 0.5,
    "mem": 128.0,
    "ports": [
        11000
    ],
    "requirePorts": false,
    "instances": 1,
    "executor": "",
    "container": null,
    "env": {},
    "constraints": [],
    "labels": {},
    "uris": [
        "https://github.com/andygrunwald/simple-webserver/releases/download/v0.0.1/simple-webserver-v0.0.1-linux-amd64.tar.gz"
    ],
    "dependencies": [],
    "healthChecks": [
        {
            "protocol": "HTTP",
            "path": "/ping",
            "gracePeriodSeconds": 10,
            "intervalSeconds": 60,
            "portIndex": 0,
            "timeoutSeconds": 10,
            "maxConsecutiveFailures": 3
        }
    ],
    "backoffSeconds": 1,
    "backoffFactor": 1.15,
    "maxLaunchDelaySeconds": 3600,
    "upgradeStrategy": {
        "minimumHealthCapacity": 1,
        "maximumOverCapacity": 1
    }
}
